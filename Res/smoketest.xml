<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="2" parallel="classes" configfailurepolicy="continue"
skipfailedinvocationcounts="true" preserve-order="true">
  <listeners>
<listener class-name="com.netease.qa.testng.TestResultListener" />
<listener class-name="com.netease.qa.testng.RetryListener" />
<listener class-name="com.netease.qa.testng.PowerEmailableReporter" />
    </listeners>
  <test name="Test">
    <classes>
      <class name="spctest.com.pageAction.NewProjectTest"/>
      <class name="spctest.com.pageAction.DataImportTest"/>
       <class name="spctest.com.pageAction.spcTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
